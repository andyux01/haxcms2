{
    "name": "silex/silex",
    "description": "The PHP micro-framework based on the Symfony Components",
    "keywords": ["microframework"],
    "homepage": "http://silex.sensiolabs.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabien Potencier",
            "email": "fabien@symfony.com"
        },
        {
            "name": "Igor Wiedler",
            "email": "igor@wiedler.ch"
        }
    ],
    "require": {
        "php": ">=5.3.9",
        "pimple/pimple": "~3.0",
        "symfony/event-dispatcher": "7.0.7",
        "symfony/http-foundation": "7.0.7",
        "symfony/http-kernel": "7.0.7",
        "symfony/routing": "7.0.7"
    },
    "require-dev": {
        "symfony/security": "4.4.50",
        "symfony/config": "7.0.7",
        "symfony/intl": "7.0.7",
        "symfony/form": "7.0.7",
        "symfony/browser-kit": "7.0.7",
        "symfony/css-selector": "7.0.7",
        "symfony/debug": "4.4.44",
        "symfony/dom-crawler": "7.0.7",
        "symfony/finder": "7.0.7",
        "symfony/monolog-bridge": "7.0.7",
        "symfony/options-resolver": "7.0.7",
        "symfony/phpunit-bridge": "~7.0",
        "symfony/process": "7.0.7",
        "symfony/serializer": "7.0.7",
        "symfony/translation": "7.0.7",
        "symfony/twig-bridge": "7.0.7",
        "symfony/validator": "7.0.7",
        "twig/twig": "3.10.1",
        "doctrine/dbal": "~4.0",
        "swiftmailer/swiftmailer": "~6",
        "monolog/monolog": "^3.0.0"
    },
    "autoload": {
        "psr-4": { "Silex\\": "src/Silex" }
    },
    "autoload-dev" : {
        "psr-4": { "Silex\\Tests\\" : "tests/Silex/Tests" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3.x-dev"
        }
    },
    "minimum-stability": "dev"
}
