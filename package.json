{
  "name": "haxcms",
  "version": "2.0.9",
  "description": "CMS backend and rigging for a full on CMS",
  "main": "../index.html",
  "repository": {
    "type": "git",
    "url": "git@github.com/elmsln/haxcms.git"
  },
  "author": {
    "name": "btopro"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@lrnwebcomponents/baseline-build-hax": "^8.0.0",
    "@lrnwebcomponents/elmsln-apps": "^8.0.0",
    "@lrnwebcomponents/haxcms-elements": "^8.0.0"
  },
  "devDependencies": {
    "@open-wc/building-utils": "3.0.1",
    "@prettier/plugin-php": "0.22.2",
    "glob": "10.3.10",
    "gulp": "4.0.2",
    "gulp-minify-html-literals": "1.1.7",
    "gulp-terser": "2.1.0",
    "patch-package": "8.0.0",
    "polymer-build": "3.1.4",
    "polymer-cli": "1.9.11",
    "prettier": "3.2.5"
  },
  "scripts": {
    "lint": "prettier --check 'system/**/*.php' '*.{html,js,php,yml}'",
    "format": "prettier --write 'system/**/*.php' '*.{html,js,php,yml}'",
    "build": "ddev rm --all && polymer build && yarn run terser && yarn run wc-autoloader && yarn run version-match && ddev start",
    "postinstall": "./node_modules/.bin/patch-package",
    "terser": "gulp terser --gulpfile=gulpfile.cjs",
    "wc-autoloader": "gulp wc-autoloader --gulpfile=gulpfile.cjs",
    "version-match": "gulp version-match --gulpfile=gulpfile.cjs",
    "start": "yarn run dev",
    "dev": "polymer serve --open --entrypoint dev.html --allow-origin --open-path=/",
    "nodeapp": "cd system/backend/nodejs && code . && yarn start"
  },
  "npmClient": "yarn",
  "useWorkspaces": true,
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "resolutions": {
    "@vaadin/router": "1.7.5",
    "mobx": "6.12.0",
    "lit": "3.1.2",
